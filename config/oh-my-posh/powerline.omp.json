          "background": "#4d4d4d",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "properties": {
            "display_mode": "context",
            "missing_command_text": "pnpm not found",
            "extensions": [
              "pnpm-lock.yaml",
