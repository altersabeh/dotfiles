        },
        {
          "foreground": "#ff6a00",
          "background": "#5a3a73",
          "style": "diamond",
          "trailing_diamond": "",
          "template": "{{ if .Root }} 󰢏 {{ else }} 󰒃 {{ end }}",
          "type": "session"
        },
        {
          "background": "#ff479c",
          "foreground": "#ffffff",
          "style": "diamond",
          "trailing_diamond": " ",
