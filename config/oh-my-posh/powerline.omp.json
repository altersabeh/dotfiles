        },
        {
          "foreground": "#998e7d",
          "background": "#fbf0df",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "properties": {
            "display_mode": "context",
            "missing_command_text": "bun not found",
            "extensions": [
              "bun.lock",
              "bun.lockb"
            ]
          },
          "template": "</>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} </>",
