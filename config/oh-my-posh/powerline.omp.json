{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ffff55",
          "foreground": "#000000",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
