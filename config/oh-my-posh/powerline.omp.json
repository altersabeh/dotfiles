        },
        {
          "foreground": "#2e2a65",
          "background": "#2188b6",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "properties": {
            "display_mode": "context",
            "missing_command_text": "yarn not found",
            "extensions": [
              "yarn.lock"
            ]
          },
          "template": "</>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} </>",
          "type": "yarn"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#88c0d0",
          "style": "plain",
          "template": " 󱇬 ",
          "type": "text"
        },
        {
          "background": "#88c0d0",
          "foreground": "#2e3440",
          "properties": {
            "home_icon": "󰋜",
            "folder_separator_icon": " 󰁕 ",
            "style": "agnoster_full"
          },
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 󰉋 {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground_templates": [
            "{{ if .Writable }}#00ff11{{ else }}#ff0000{{ end }}"
          ],
          "background": "#506da6",
          "style": "diamond",
          "trailing_diamond": "",
          "template": "{{ if .Writable }}  {{ else }}  {{ end }}",
          "type": "path"
        },
        {
          "foreground": "#FF9248",
          "background": "#112856",
          "style": "diamond",
          "trailing_diamond": "",
          "template": " 󱐋 ",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background_templates": [
            "{{ if eq .OS \"linux\" }}#D80150{{ end }}",
            "{{ if eq .OS \"darwin\" }}#FF605C{{ end }}",
            "{{ if eq .OS \"windows\" }}#11B7EB{{ end }}"
          ],
          "foreground": "#433a3a",
          "properties": {
            "linux": " \uF17C Linux </>",
            "macos": " \uF179 macOS </>",
            "windows": " \uF17A Windows </>"
          },
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#94bf19{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FFFB38{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "foreground": "#000000",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": " ",
            "charged_icon": " ",
            "not_charging_icon": " "
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}% ",
          "type": "battery"
        },
        {
          "foreground": "#2c3436",
          "background": "#55ffff",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "time_format": "Mon Jan 02, 2006 15:04"
          },
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#ff00ea",
          "foreground": "#000000",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .Name }} ",
          "type": "shell"
        },
        {
          "background": "#4d342f",
          "foreground": "#FF9248",
          "type": "executiontime",
          "style": "diamond",
          "trailing_diamond": " ",
          "template": "  {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          }
        },
        {
          "foreground": "#f4a616",
          "background": "#ea1e17",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": "  {{ reason .Code }} ",
          "type": "status"
        },
        {
          "foreground": "#88c0d0",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "[<#b48ead> {{ .HEAD }} {{ .BranchStatus }}{{ if .Staging.Changed }}<#00AA00> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> \u25cf {{ .Working.String }}</>{{ end }}<#b48ead></> ] ",
          "type": "git"
        },
        {
          "foreground": "#88c0d0",
          "type": "project",
          "style": "plain",
          "template": "[<#a39977> {{.Type}}  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}{{.Version}} {{ end }}{{ if .Name }}{{ .Name }} {{ end }}{{ end }}<#a39977></>]"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "spotify",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#282828",
          "background": "#1ed761",
          "properties": {
            "playing_icon": "󰐊 ",
            "paused_icon": "󰏤 ",
            "stopped_icon": "󰓛 "
          },
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} "
        },
        {
          "type": "ytm",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#eadada",
          "background": "#CD201F",
          "properties": {
            "playing_icon": "󰐊 ",
            "paused_icon": "󰏤 ",
            "stopped_icon": "󰓛 "
          },
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} "
        }
      ],
      "type": "prompt"
    },
    {
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ecf7fa",
          "foreground_templates": [ "{{ if gt .Code 0 }}#ef5350{{ end }}" ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "  ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "style": "plain",
    "foreground": "#ecf7fa",
    "template": "  "
  },
  "transient_prompt": {
    "style": "plain",
    "foreground_templates": [ "{{ if gt .Code 0 }}#ef5350{{ else }}#04c738{{ end }}" ],
    "template": "  "
  },
  "valid_line": {
    "style": "plain",
    "foreground": "#04c738",
    "template": "  "
  },
  "error_line": {
    "style": "plain",
    "foreground": "#ef5350",
    "template": "  "
  },
  "version": 3
}
